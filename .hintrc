{
  "extends": [
    "development"
  ],
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_chr <= 130",
    "not chrome <= 130",
    "not edge <= 130",
    "not firefox <= 132",
    "not safari <= 18.1",
    "not samsung <= 26"
  ],
  "hints": {
    "compat-api/css": [
      "default",
      {
        "ignore": [
          "text-align: -webkit-match-parent"
        ]
      }
    ],
    "axe/name-role-value": [
      "default",
      {
        "button-name": "off",
        "link-name": "off"
      }
    ],
    "axe/aria": [
      "default",
      {
        "aria-required-children": "off"
      }
    ],
    "axe/forms": [
      "default",
      {
        "select-name": "off"
      }
    ],
    "no-inline-styles": "off"
  }
}